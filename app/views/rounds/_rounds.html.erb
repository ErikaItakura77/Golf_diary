<ul class="media-list">
  <% rounds.each do |round| %>
    <li class="media">
      <div class="media-left">
        <img class="media-object img-rounded" src="<%= gravatar_url(round.user, { size: 50 }) %>" alt="">
      </div>
      <div class="media-body">
        <div>
          <%= link_to round.user.name, user_path(round.user) %> <span class="text-muted">posted at <%= round.created_at %></span>
        </div>
        <div>
          <p><%= round.day %></p>
          <p><%= round.course %></p>
          <p><%= round.weather %></p>
          <p><%= round.wind %></p>
          <p><%= round.green %></p>
          <p><%= round.tee %></p>
          <p><%= round.score_first_nine %></p>
          <p><%= round.score_back_nine %></p>
          <p><%= round.score_add %></p>
          <p><%= round.member1 %></p>
          <p><%= round.member2 %></p>
          <p><%= round.member3 %></p>
          <p><%= round.member4 %></p>
          <p><%= round.picture %></p>
          <p><%= round.video %></p>
        </div>
      </div>
          <% if current_user == round.user %>
            <%= link_to "Delete", round, method: :delete, data: { confirm: "You sure?" }, class: 'btn btn-danger btn-xs' %>
          <% end %>
    </li>
  <% end %>
  <%= paginate rounds %>
</ul>